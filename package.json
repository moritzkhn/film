{
  "name": "film",
  "version": "1.0.0",
  "description": "Beispiel",
  "author": "Moritz KÃ¶hn",
  "private": true,
  "license": "GPL-3.0",
  "exports": "./dist/src/main.js",
  "type": "module",
  "engines": {
    "node": "17.8.0",
    "npm": ">=8.5.5"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "node dist/src/main.js",
    "prestart:dev": "npm run copy-resources",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test:ORIG": "jest",
    "pretest": "npm run copy-resources",
    "test": "jest --runInBand --forceExit --expand",
    "test:watch": "jest --watch",
    "test:coverage": "npm t -- --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "asciidoc": "node scripts/asciidoc",
    "clinic": "clinic doctor -- node dist/src/main.js",
    "clean": "rimraf dist log/server*.log",
    "depcheck": "depcheck",
    "deploy:heroku": "git push heroku main",
    "deploy:prepare": "concurrently --prefix \"[{name}]\" --names \"ESLint,copy-resources,TypeScript\" --prefix-colors \"yellow.bold,gray.bold,cyan.bold,green.bold\" \"npm run eslint\" \"npm run copy-resources\" \"npm run tsc\"",
    "eslint": "eslint --ext .ts ./src ./__tests__",
    "copy-resources": "node scripts/copy-resources",
    "leasot": "leasot --exit-nicely src/**/*.ts",
    "heroku:browser": "heroku open --app protected-eyrie-38026 /api/000000000000000000000001",
    "heroku:browser:file": "heroku open --app protected-eyrie-38026 /curl --include --tlsv1.2 --insecure https://protected-eyrie-38026.herokuapp.com/file/000000000000000000000001",
    "heroku:curl": "curl --include --tlsv1.2 --insecure https://protected-eyrie-38026.herokuapp.com/api/000000000000000000000001",
    "prepack": "npm run copy-resources",
    "pack": "pack build juergenzimmermann/buch:1.0.0 --builder paketobuildpacks/builder:base",
    "pack:clear-cache": "pack build juergenzimmermann/buch:1.0.0 --builder paketobuildpacks/builder:base --clear-cache",
    "pack:inspect": "pack inspect juergenzimmermann/buch:1.0.0",
    "prettier": "prettier --write \"src/**/*.ts\" \"__tests__/**/*.ts\"",
    "prettier:check": "prettier --check \"src/**/*.ts\" \"__tests__/**/*.ts\"",
    "revealjs": "asciidoctor-revealjs extras/doc/folien/folien.adoc",
    "rmi": "docker rmi juergenzimmermann/buch:1.0.0",
    "snyk:auth": "echo https://app.snyk.io/account && snyk auth",
    "snyk": "snyk test",
    "typedoc": "typedoc",
    "tsc": "tsc --pretty",
    "tsc:watch": "npm run tsc -- --watch",
    "heroku-postbuild-audit": "npm audit --omit dev && tsc --version && tsc && node scripts/copy-resources no-tls",
    "heroku-postbuild": "tsc --version && tsc && node scripts/copy-resources no-tls"
  },
  "keywords": [
    "Node",
    "npm",
    "Nest",
    "REST",
    "GraphQL",
    "Apollo",
    "Axios",
    "TypeScript",
    "ESLint",
    "MongoDB",
    "mongoose",
    "JWT",
    "Passport",
    "bcrypt",
    "Ajv",
    "CSP",
    "Clickjacking",
    "CORS",
    "winston",
    "nodemailer",
    "Jest",
    "ts-jest"
  ],
